{% extends 'base.html.twig' %}
{% block title %} Test {% endblock title %}
    {% block content %}
        <div class="container-flex">
            <div class="row">
                {%for product in array %}
                    <div class="col-4">
                        <div class="card" style="width: 18rem;">
                            <div class="card-body">
                                <h5 class="card-title">{{ product }}</h5>
                            </div>
                        </div>
                    </div>
                {% endfor %}
            </div>
        </div>
    {% endblock content %}